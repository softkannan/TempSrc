<Window x:Class="RemoteClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid Margin="0,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Menu Margin="10,0,466,0" Grid.Row="0">
            <MenuItem Header="File">
                <MenuItem Header="Cancel" HorizontalAlignment="Left" Width="140" Click="MenuItem_Click"/>
            </MenuItem>
        </Menu>
        <ListBox x:Name="listOut" ItemsSource="{Binding Values}" Margin="10,0,10,10" Grid.Row="1"/>
    </Grid>
</Window>
